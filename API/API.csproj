<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>2f95c325-82bd-4876-b30f-ea41e800a3b3</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
    <Company />
    <Authors>Moritz JÃ¶kel</Authors>
    <Product>Credential Leakage Monitoring API</Product>
    <AssemblyName>CredentialLeakageMonitoring.$(MSBuildProjectName)</AssemblyName>
    <RootNamespace>CredentialLeakageMonitoring.$(MSBuildProjectName.Replace(" ", "_"))</RootNamespace>
    <DockerfileContext>.</DockerfileContext>
  </PropertyGroup>


  <ItemGroup>
